{
  "name": "greener-cicd",
  "version": "1.0.0",
  "description": "Greener CI/CD - Cloudflare Worker with D1 Database",
  "type": "module",
  "scripts": {
    "setup": "node scripts/setup-d1.js",
    "setup:prod": "node scripts/setup-d1.js --env=production",
    "migrate": "node scripts/setup-d1.js --migrate-only",
    "migrate:prod": "node scripts/setup-d1.js --migrate-only --env=production",
    "bundle": "esbuild proxy/worker.js --bundle --format=esm --platform=neutral --outfile=proxy/worker-bundled.js --external:cloudflare:* --minify",
    "bundle:migration": "esbuild proxy/migration-worker.js --bundle --format=esm --platform=neutral --outfile=proxy/migration-worker-bundled.js --external:cloudflare:* --minify",
    "dev": "wrangler dev",
    "deploy": "npm run bundle && wrangler deploy",
    "deploy:prod": "npm run bundle && wrangler deploy --env production",
    "deploy:migration": "npm run bundle:migration && wrangler deploy proxy/migration-worker-bundled.js --name greener-cicd-migrations",
    "tail": "wrangler tail",
    "tail:prod": "wrangler tail --env production"
  },
  "dependencies": {},
  "devDependencies": {
    "wrangler": "^3.0.0",
    "esbuild": "^0.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greener-hayden/greenerCICD.git"
  },
  "keywords": [
    "cicd",
    "cloudflare-workers",
    "d1-database",
    "github-actions"
  ],
  "author": "greener-hayden",
  "license": "MIT"
}